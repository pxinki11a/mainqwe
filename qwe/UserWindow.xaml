<Window x:Class="qwe.UserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:qwe"
        mc:Ignorable="d"
        Icon="images\logo.png"
        Title="Окно пользователя" WindowStartupLocation="CenterScreen" Height="900" Width="1800" Background="#cbff7b" ResizeMode="NoResize" ShowInTaskbar="False">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="25"/>
        </Grid.RowDefinitions>
        <TextBlock FontSize="24" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Row="0" Text="Резервация кабинетов"/>
        <Frame Grid.Row="1" NavigationUIVisibility="Hidden" Name="UserFrame" />
        <Button Grid.Row="0" HorizontalAlignment="Left" Margin="56,15,0,25" Content="Назад" x:Name="BtnBack" Click="BtnBack_Click" />
    </Grid>
</Window>
