<Window x:Class="qwe.AuthWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:qwe"
        Icon="images\logo.png"
        mc:Ignorable="d"
        Title="Авторизация" WindowStartupLocation="CenterScreen" Height="900" Width="1800" Background="#040C0E">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="35"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="35"/>
        </Grid.RowDefinitions>

        <Frame Grid.Row="1" NavigationUIVisibility="Hidden" Name="AuthFrame"/>
    </Grid>
</Window>
